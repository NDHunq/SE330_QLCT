{
  "name": "inversify-express",
  "version": "1.0.0",
  "description": "- Flow: Controller --> Service --> Repository --> TypeORM Entity",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && tsc-alias",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/database/migrations/$npm_config_name",
    "migration:generate": "npm run build && typeorm-ts-node-commonjs migration:generate src/database/migrations/migrations -d dist/database/db.datasource.js",
    "migration:run": "npm run build && typeorm-ts-node-commonjs migration:run -d dist/database/db.datasource.js",
    "migration:sync": "npm run build && typeorm-ts-node-commonjs schema:sync -d dist/database/db.datasource.js",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d dist/database/db.datasource.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.2.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20"
  }
}
